{% extends 'base.html.twig' %}

{% block title %}Создание пользователя{% endblock %}

{% block breadcrumbs %}
    <ol class="breadcrumb">
        <li class="breadcrumb-item"><a href="{{ path('list') }}">List</a></li>
        <li class="breadcrumb-item"><a href="{{ path('create') }}">Create</a></li>
        <li class="breadcrumb-item active">Create</li>
    </ol>
{% endblock %}

{% block body %}

    <div class="card">
        <div class="card-body">
            {{ form_start(form) }}
            {{ form_widget(form) }}
            <button type="submit" class="btn btn-primary">Create</button>
            {{ form_end(form) }}
        </div>
    </div>

{% endblock %}